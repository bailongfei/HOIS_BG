<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xh.dao.QueueSrvGroup">

    <!-- 查询服务项 -->
    <select id="queueSrvGroup" resultType="map"   statementType="CALLABLE" >
		exec queueSrvGroup

	</select>
   <!--查询业务-->
   <select id="queueByCondition" parameterType="map" resultType="map" statementType="CALLABLE">
       exec Queue_WS
       #{Customer,mode=IN,jdbcType=VARCHAR},
       #{QueueNo,mode=IN,jdbcType=VARCHAR},
       #{StatusType,mode=IN,jdbcType=VARCHAR}
   </select>
    <select id="QueueAct" parameterType="map" resultType="map" statementType="CALLABLE">
        exec SP_MS_Queue_Act
        #{Data_ID,mode=IN,jdbcType=NUMERIC}
    </select>
</mapper>